{
	"name" : "__MSG_extName__",
	"version" : "2.3.3",
	"description" : "__MSG_extDescription__",
	"default_locale" : "ru",
	"icons" : {
		"128" : "pic/icon128.png",
		"48" : "pic/icon48.png",
		"16" : "pic/icon16.png"
	},
	"manifest_version" : 2,
	"browser_action" : {
		"default_icon": "pic/icon19.png",
		"default_title": "__MSG_extName__"
	},
	"background": {
        "scripts": ["js/settings.js", "js/utils.js", "js/background.js"]
    },
    "offline_enabled" : false,
    "options_page" : "options.html",
    "minimum_chrome_version" : "20",
    "permissions" : ["background", "unlimitedStorage", "tabs", "notifications", "*://*.vk.com/*"],
    "content_scripts" : [{
		"matches" : ["*://*.vk.com/*"],
		"js" : ["js/contentscript.js"]
	}]
}
